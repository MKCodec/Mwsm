{
  "apps": [
    {
      "name": "Bot-Mwsm",
      "script": "mwsm.js",
      "cwd": "/var/api/Mwsm/",
      "watch": ["mwsm.js"],
      "ignore_watch": ["mwsm.db"],
      "watch_options": {
        "followSymlinks": false
      }
    },
    {
      "name": "Ask-Mwsm",
      "script": "mwsm.py",
      "cwd": "/var/api/Mwsm/",
      "interpreter": "python3",
      "watch": ["mwsm.py"],
      "ignore_watch": ["mwsm.db"],
      "watch_options": {
        "followSymlinks": false
      }
    },
    {
      "name": "Honeygain",
      "script": "/usr/local/bin/honeygain-cli",
      "args": [
        "-tou-accept",
        "-email", "${HG_EMAIL}",
        "-pass", "${HG_PASS}",
        "-device", "Mwsm-Docker"
      ],
      "autorestart": true,
      "restart_delay": 5000
    }
  ]
}
